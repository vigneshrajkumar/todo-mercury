<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="125.0" prefWidth="290.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.SettingsController">

    <VBox alignment="CENTER">
        <HBox alignment="CENTER">
            <Label text="Stay on top">
            <HBox.margin>
               <Insets bottom="5.0" left="5.0" right="20.0" top="5.0" />
            </HBox.margin></Label>
            <CheckBox fx:id="vStayOnTopCheckBox" />
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
        </HBox>

        <HBox alignment="CENTER">
            <Button fx:id="vResetToDefaultSettings" text="Reset To Defaults">
            <HBox.margin>
               <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
            </HBox.margin></Button>
            <Button fx:id="vSaveSettings" text="Save Settings">
            <HBox.margin>
               <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
            </HBox.margin></Button>
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
        </HBox>
      <padding>
         <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
      </padding>
    </VBox>

</AnchorPane>
